
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.vinksel.ultimateprojectofdoom3000lesdeuxtours.entities">

	<class name="Session" table="session">
		<id name="id" column="id">
		    <generator class="identity"/>
		</id>

		
		<property name="start_date" column="start_date" type="date" not-null="true" />
		<property name="end_date" column="end_date" type="date" not-null="true" />
		<property name="max_students" column="max_students" type="integer" not-null="true" />
		<many-to-one name="Location" class="org.vinksel.ultimateprojectofdoom3000lesdeuxtours.entities.Location"
            column="location_id" unique="false" not-null="true"
            cascade="all" />
		<many-to-one name="Course" class="org.vinksel.ultimateprojectofdoom3000lesdeuxtours.entities.Course"
            column="course_id" unique="false" not-null="true"
            cascade="all" />
	</class>
</hibernate-mapping>